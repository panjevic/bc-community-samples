<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DeliveryApp.DeliveryPage"
             xmlns:pages="clr-namespace:DeliveryApp"
             xmlns:viewModel="clr-namespace:DeliveryApp.ViewModel;assembly=DeliveryApp">
  <MasterDetailPage.Master>
    <pages:DeliveryPageMaster x:Name="MasterPage"/>
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:DeliveryPageDetail />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>